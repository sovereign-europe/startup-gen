{
  "name": "startup-gen",
  "version": "1.5.0",
  "description": "A CLI tool for early-stage startups to build lean startup methodology",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "startup": "./dist/index.js"
  },
  "scripts": {
    "prepare": "npm run build && husky",
    "build": "vite build",
    "dev": "vite-node -w ./src/index.tsx",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix",
    "start": "node dist/index.js",
    "semantic-release": "semantic-release",
    "format": "prettier --write src/.",
    "format:check": "prettier --check src/.",
    "eval:customer-persona": "npx -y promptfoo@latest eval -c prompts/promptfoo/customer-persona.yaml"
  },
  "keywords": [
    "cli",
    "startup",
    "lean-startup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/mistral": "^1.2.8",
    "@ai-sdk/openai": "^1.3.23",
    "@inkjs/ui": "^2.0.0",
    "@types/chokidar": "^1.7.5",
    "@types/react": "^19.1.8",
    "ai": "^4.3.17",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "dotenv": "^16.6.0",
    "fs-extra": "^11.3.0",
    "ink": "^6.0.1",
    "inquirer": "^12.6.3",
    "marked-terminal": "^7.3.0",
    "md-to-pdf": "^5.2.4",
    "meow": "^13.2.0",
    "mustache": "^4.2.0",
    "react": "^19.1.0",
    "zod": "^3.25.73"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/marked-terminal": "^6.1.1",
    "@types/mustache": "^4.2.6",
    "@types/node": "^24.0.4",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "3.6.2",
    "semantic-release": "^24.2.5",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.0.2",
    "vite-node": "^3.2.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
